root = true

[*]
charset = utf-8
indent_style = tab
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

[*.sln]
indent_style = space
indent_size = 4

[*.csproj]
indent_style = space
indent_size = 2

[*.fs]
indent_style = space
indent_size = 4

[*.fsx]
indent_style = space
indent_size = 4
